<p id="notice"><%= notice %></p>
  
    <table class="table table-striped table-hover">
				<thead>
					<tr>
						<th>Project Name</th>
						<th>Assigned to</th>
						<th>Status</th>
						<th>Duration</th>
						<th>Actions</th>
					</tr>
				</thead>
				<tbody>
                
<% @projects.each do |project| %>
					<tr>
						<td><%= project.project_name %></td>
            <td><%= project.employee.name %></td>
						<td><%= project.status %></td>
						<td><%= project.duration %></td>
						<td>
                        	<a href="<%= project_path(project) %>" class="edit" ><i class="material-icons" data-toggle="tooltip" title="Edit">&#xf1c5;</i></a>
							<a href="<%= edit_project_path(project) %>" class="edit" ><i class="material-icons" data-toggle="tooltip" title="Edit">&#xE254;</i></a>
							<%= link_to 'Delete', project, method: :delete, data: { confirm: 'Are you sure?' }, :class => "delete" %>
						</td>
					</tr>
					<% end %>
				</tbody>
			</table>         
           
     </div>


