 <p id="notice"><%= notice %></p>    
  
            <table class="table table-striped table-hover">
				<thead>
					<tr>
						<th>Emp Id</th>
						<th>Name</th>
						<th>Email</th>
						<th>Address</th>
						<th>Phone</th>
						<th>Actions</th>
					</tr>
				</thead>
				<tbody>
                
				<% @employees.each do |employee| %>
					<tr>
						<td><%= employee.emp_id %></td>
						<td><%= employee.name %></td>
						<td><%= employee.email %></td>
						<td><%= employee.designation %></td>
						<td><%= employee.phone %></td>
						<td>
                        	<a href="<%= employee_path(employee) %>" class="edit" ><i class="material-icons" data-toggle="tooltip" title="Edit">&#xf1c5;</i></a>
							<a href="<%= edit_employee_path(employee) %>" class="edit" ><i class="material-icons" data-toggle="tooltip" title="Edit">&#xE254;</i></a>
							<%= link_to 'Delete', employee, method: :delete, data: { confirm: 'Are you sure?' }, :class => "delete" %>
						</td>
					</tr>
					<% end %>
				</tbody>
			</table>         
           
     </div>


  




